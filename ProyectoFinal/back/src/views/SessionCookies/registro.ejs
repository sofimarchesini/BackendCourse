  <body>
    <div style="text-align: center;margin-top:130px">
        <h1>REGISTRO</h1>
        <form action="/registro" method="post">
        <input type="text" name="nombre" placeholder="Ingresa tu usuario" />
        <input type="email" name="email" placeholder="Ingresa tu correo" />
        <input
            type="password"
            name="password"
            placeholder="Ingresa tu contrasena"
        />
        <input type="tel" name="telefono" placeholder="Telefono">
        <input type="text" name="mensaje" placeholder="Escribe tu Mensaje">
        <input id="photo" name='photo' placeholder="foto" class="form-control" type="file" required>
        <button onclick="location.href = '/login'"></button>>Enviar</button>
        </form>
    </div>
  </body>
</html>